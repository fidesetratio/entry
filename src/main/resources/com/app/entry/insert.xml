<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace='com.app.dao.BacDao'>
  	<insert id="insertMstAgenTemp" parameterType="java.util.HashMap">
		INSERT INTO EKA.MST_AGENT_TEMP
			(REG_SPAJ,MAT_NAMA_AGENT)
		VALUES
			(#{strTmpSPAJ},#{v_stragentnama})
	</insert>
	
	
	<insert id="insertMstClientTtg" parameterType="com.ekalife.elions.model.Tertanggung">
		INSERT INTO EKA.MST_CLIENT_NEW (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
		MCL_ID , MCL_JENIS, MCL_FIRST                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
		, MCL_BLACKLIST, MCL_TGL_INPUT, LUS_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
		, MCL_DATA_SALAH , MCL_GELAR, LSIDE_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
		, MSPE_NO_IDENTITY 
		, MSPE_NO_IDENTITY_EXPIRED
		, LSED_ID, LGJ_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
		, LJB_ID, MPN_JOB_DESC, MSPE_MOTHER                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
		, MSPE_SEX, MSPE_DATE_BIRTH, MSPE_PLACE_BIRTH                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
		, LSAG_ID , MSPE_STS_MRT , LSNE_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
		, MSPE_LAMA_KERJA, MSPE_EMAIL, MKL_TUJUAN                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
		, MKL_PENGHASILAN , MKL_PENDANAAN , MKL_SMBR_PENGHASILAN, MKL_KERJA,  MKL_INDUSTRI     , lti_id, mcl_agama
		, MKL_DANA_FROM, MKL_HASIL_FROM, MKL_SMBR_HASIL_FROM, MKL_SUMBER_PREMI, LSRE_ID_PREMI, MKL_RED_FLAG
		, MCL_FIRST_ALIAS,MCL_GREEN_CARD,MCL_COMPANY_NAME,MKL_KERJA_KET,MCL_NPWP
		) values (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
		 upper(#{mcl_id}),  0,  upper(#{mcl_first})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
		,  0,  SYSDATE,  (#{lus_id})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
		,  NULL,  upper(#{mcl_gelar}),  (#{lside_id})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
		,  upper(trim(replace(replace(replace(replace(replace(replace(#{mspe_no_identity#,'/'),'.'),','),'\'),'-'),' ')))
		,  (#{mspe_no_identity_expired})
		,  (#{lsed_id}),  upper(#{lgj_id})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
		,  upper(#{ljb_id}),  upper(#{mpn_job_desc}),  upper(#{mspe_mother})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
		,  (#{mspe_sex}),  (#{mspe_date_birth}),  upper(#{mspe_place_birth})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
		,  (#{lsag_id}),  upper(#{mspe_sts_mrt}),  (#{lsne_id})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
		,  NULL,  upper(#{email}),  upper(#{mkl_tujuan})                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
		,  upper(#{mkl_penghasilan}), upper(#{mkl_pendanaan}) , upper(#{mkl_smbr_penghasilan}), upper(#{mkl_kerja}), upper(#{mkl_industri})       , #{lti_id:INTEGER}, upper(#{mcl_agama})
		,  (#{mkl_dana_from}), (#{mkl_hasil_from}), (#{mkl_smbr_hasil_from}), (#{mkl_sumber_premi}), (#{lsre_id_premi}), (#{mkl_red_flag})
		, upper(#{mcl_first_alias}),(#{mcl_green_card}),upper(#{mcl_company_name}),upper(#{mkl_kerja_ket}),(#{mcl_npwp})
		)           
	</insert>	
	
	
</mapper>